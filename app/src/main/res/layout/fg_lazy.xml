<?xml version="1.0" encoding="utf-8"?>
<me.henrytao.smoothappbarlayout.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/nested_scroll_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp">

        <include
            android:id="@+id/item_header_view_pager_parallax_spacing"
            layout="@layout/item_header_view_pager_parallax_spacing" />

        <TextView
            android:id="@+id/lazy_tv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text=""
            android:textSize="50dp" />

    </LinearLayout>


</me.henrytao.smoothappbarlayout.widget.NestedScrollView>